    "leaflet-heatmap": {
      "depends": [
        "leaflet:L"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~2.0.1",
    "browserify": "~8.1.3",
    "chai": "^1.10.0",
    "gulp": "^3.8.11",
    "gulp-changed": "^1.1.1",
    "gulp-imagemin": "^2.2.0",
    "gulp-jshint": "^1.9.2",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^1.3.3",
    "gulp-util": "^3.0.3",
    "karma": "^0.12.31",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-firefox-launcher": "^0.1.4",
