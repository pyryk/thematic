{
  "name": "geoviz-framework",
  "version": "0.0.1",
  "description": "A framework for visualizing geographical data on the web.",
  "repository": {
    "type": "git",
    "url": "git://github.com/pyryk/geoviz-framework.git"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js",
    "heatmap": "./src/javascript/lib/heatmap.js",
    "leaflet-heatmap": "./src/javascript/lib/leaflet-heatmap.js",
    "d3-contour": "./src/javascript/lib/d3.geom.contour.js",
    "vizframe": "./src/javascript/vizframe/vizframe.js",
    "imodule": "./src/javascript/vizframe/modules/imodule.js",
    "vizframe-heatmap": "./src/javascript/vizframe/modules/heatmap.js",
    "vizframe-isarithmic": "./src/javascript/vizframe/modules/isarithmic.js",
    "vizframe-contour": "./src/javascript/vizframe/modules/contour.js",
    "vizframe-dot-choropleth": "./src/javascript/vizframe/modules/dot-choropleth.js",
    "vizframe-dot-choropleth-aggregate": "./src/javascript/vizframe/modules/dot-choropleth-aggregate.js",
    "vizframe-dot": "./src/javascript/vizframe/modules/dot.js",
    "vizframe-choropleth": "./src/javascript/vizframe/modules/choropleth.js",
    "average-aggregator": "./src/javascript/vizframe/utils/average-aggregator.js",
    "converters": "./src/javascript/vizframe/utils/converters.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "cssify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "heatmap": {
      "exports": "h337",
      "depends": [
        "leaflet:L"
      ]
    },
    "leaflet-heatmap": {
      "exports": "HeatmapOverlay",
      "depends": [
        "leaflet:L",
        "heatmap:h337"
      ]
    },
    "d3-contour": {
      "exports": "d3",
      "depends": [
        "d3:d3"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~0.8.2",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "coffeeify": "~0.6.0",
    "cssify": "^0.5.1",
    "gulp": "^3.8.0",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~1.3.2",
    "pretty-hrtime": "~0.2.1",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "d3": "^3.4.11",
    "jquery": "~2.1.0",
    "leaflet": "^0.7.3",
    "q": "^1.0.1",
    "turf": "^1.0.0"
  }
}
