{
  "name": "geoviz-framework",
  "version": "0.0.1",
  "description": "A framework for visualizing geographical data on the web.",
  "repository": {
    "type": "git",
    "url": "git://github.com/pyryk/geoviz-framework.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js",
    "heatmap": "./src/javascript/lib/heatmap.js",
    "leaflet-heatmap": "./src/javascript/lib/leaflet-heatmap.js",
    "d3-contour": "./src/javascript/lib/d3.geom.contour.js",
    "thematic": "./src/javascript/thematic/thematic.js",
    "imodule": "./src/javascript/thematic/modules/imodule.js",
    "index": "./src/javascript/thematic/index.js",
    "thematic-heatmap": "./src/javascript/thematic/modules/heatmap.js",
    "thematic-isarithmic": "./src/javascript/thematic/modules/isarithmic.js",
    "thematic-contour": "./src/javascript/thematic/modules/contour.js",
    "thematic-dot-choropleth": "./src/javascript/thematic/modules/dot-choropleth.js",
    "thematic-dot-choropleth-aggregate": "./src/javascript/thematic/modules/dot-choropleth-aggregate.js",
    "thematic-dot": "./src/javascript/thematic/modules/dot.js",
    "thematic-choropleth": "./src/javascript/thematic/modules/choropleth.js",
    "average-aggregator": "./src/javascript/thematic/utils/average-aggregator.js",
    "converters": "./src/javascript/thematic/utils/converters.js",
    "separate-values-aggregator": "./src/javascript/thematic/utils/separate-values-aggregator.js",
    "leaflet-markercluster": "./src/javascript/lib/leaflet.markercluster.js",
    "promise-sync": "./src/javascript/thematic/utils/promise-sync.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "cssify",
      "browserify-global-shim"
    ]
  },
  "browserify-shim": {
    "d3-contour": {
      "exports": "d3",
      "depends": [
        "d3:d3"
      ]
    },
    "leaflet-markercluster": {
      "depends": [
        "leaflet:L"
      ]
    },
    "leaflet-heatmap": {
      "depends": [
        "leaflet:L"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.7.0",
    "browserify": "~6.2.0",
    "browserify-global-shim": "^1.0.0",
    "browserify-shim": "~3.8.0",
    "chai": "^1.10.0",
    "cssify": "^0.5.1",
    "gulp": "^3.8.0",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-jshint": "^1.9.0",
    "gulp-notify": "^1.4.2",
    "gulp-sass": "^1.1.0",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~1.3.2",
    "karma": "^0.12.28",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.5",
    "karma-firefox-launcher": "^0.1.3",
    "karma-mocha": "^0.1.9",
    "karma-opera-launcher": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "mocha": "^2.0.1",
    "pretty-hrtime": "~0.2.1",
    "sinon": "^1.12.1",
    "uglifyify": "^2.6.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~2.1.0"
  },
  "dependencies": {
    "d3": "^3.4.11",
    "jquery": "~2.1.0",
    "leaflet": "^0.7.3",
    "q": "^1.0.1",
    "underscore": "^1.7.0"
  }
}
